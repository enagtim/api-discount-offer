{
	"compilerOptions": {
		"target": "es6", 
		"experimentalDecorators": true, 
		"module": "commonjs",
		"moduleResolution": "node", 
		"outDir": "./dist",
		"removeComments": true, 
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true, 
		"strictPropertyInitialization": false, 
		"skipLibCheck": true
	}
}
